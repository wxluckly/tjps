body,div,ul,li{margin: 0;padding: 0}
li{list-style: none;}
html{height: 100%;}

@font-face {
  font-family:"STLITI";
  src: url("STLITI.eot");
  src: local("STLITI Regular"), local("STLITI"), url("STLITI.ttf") format("truetype"), url("STLITI.svg#STLITI") format("svg");
}

body{background:url(<%= image_url 'bg5.jpg' %>) no-repeat center bottom;background-size:cover;width: 100%;background-attachment: fixed;}
/*头部定义*/
header{width: 100%;height: 45px;line-height: 45px;background: #121617;position: fixed;top: 0;left: 0;z-index: 999;}
header .back{position: absolute;left: 20px;}
header .title{position: relative;width:100%;text-align: center;color: #cbcbcb;margin:0px;}
header .home{position:absolute;right: 20px;}

.picNav{width: 100%; margin:0px auto; position:fixed; bottom:0px;}
.picNav li{}
.picNav li.on{}
.picNav li a{font-family:STLITI;; text-decoration:none;width:23%;  margin-left:2%; line-height: 60px;  text-align: center; float: left;background-size:auto 100%; background:url(<%= image_url 'one_btn2.png' %>) no-repeat center; background-size: contain; color: #fff; font-weight: bolder;}
.picNav li a.on{background: url(<%= image_url 'one_btn1.png' %>) no-repeat center; background-size: contain;}

.container5{width: 90%; height:auto; margin:50px auto; position:relative; padding-top:50px;overflow-y:auto;}
.container6{width: 90%; margin:0px auto; position:relative; padding-top:30px;overflow-y:auto;min-height: 100%;}
	.picList { width:100%; float:left; overflow:hidden; }	
		.picList ul { margin:0px; padding:10px; list-style:none; }
			.picList ul li { height:42px; margin-bottom:25px; background:url(<%= image_url 'one_btn1.png' %>); float:none; }
			.picList ul li { font-size:14px; line-height:53px; color:#b2012f; text-align:center; font-family:"Microsoft YaHei"; font-weight:bold; }
			.picList li a{ font-family: STLITI;text-decoration:none; width:83%;  line-height:45px;  text-align: center; float: left;background-size:auto 100%; background:url(<%= image_url 'big_btn2.png' %>) no-repeat center; background-size: contain; color: #fff; font-weight: bolder;}
			.picList li a.on{ background:url(<%= image_url 'big_btn2.png' %>) no-repeat center; background-size: contain;}

			.page01_DownLeft a { color:#fff; text-decoration:none; }
			.page01_DownLeft a:hover { color:#fff; text-decoration:underline; }
			
	.canvas01{border: 1px solid #000;z-index:10;}
	.canvas02{border: 1px solid #000;z-index:20;}

.spirit{width:50px;height:50px;background-color:red;}


.bj,.db,.tj{ width:100%; text-align:center; height:auto; line-height:44px; padding:10px auto; margin:10px auto; background: url(<%= image_url 'pic1.jpg' %>) no-repeat center bottom;}
.bj a,.db a,.tj a{ color:#F00; font-size:24px;  font-family: STLITI; font-weight:bold; text-decoration:none;}


.container .art{width: 88%; margin: 50px auto;overflow:auto;}

.top_right,.top_left,.bottom_left,.bottom_right{position: absolute;width: 42px;height: 42px;}
.top_right{background: url(<%= image_url '2.png' %>) no-repeat center; top: 0; right: 0}
.top_left{background: url(<%= image_url '1.png' %>) no-repeat center; top: 0; left: 0}
.bottom_right{background: url(<%= image_url '4.png' %>) no-repeat center; bottom: 0; right: 0}
.bottom_left{background: url(<%= image_url '3.png' %>) no-repeat center; bottom: 0; left: 0}
.container p{width: 100%;color:#ab8821;line-height: 26px;font-family: 1.5em;}
.container p img{width:80%;margin: 5px auto;display: block;}

